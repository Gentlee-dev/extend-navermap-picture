{
    "manifest_version": 3,
    "name": "네이버지도 사진 확장기",
    "version": "1.0.0",
    "description": "네이버지도 사진 확장기",
    "permissions": ["activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_title": "네이버지도 사진 확장기"
    },
    "icons": {
        "16": "imgs/icon-16.png",
        "32": "imgs/icon-32.png",
        "48": "imgs/icon-48.png",
        "128": "imgs/icon-128.png"  
},
    "content_scripts": [
        {
            "matches": ["https://*.map.naver.com/*"],
            "js": ["content.js"]
        }
    ]
}